{
  "name": "with-tailwindcss",
  "version": "1.0.0",
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "lint": "eslint . --ext ts --ext tsx --ext js",
