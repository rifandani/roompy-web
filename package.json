{
  "name": "with-tailwindcss",
  "version": "1.0.0",
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@formspree/react": "^2.2.3",
    "autoprefixer": "^10.1.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "firebase": "^8.2.0",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "next": "^10.0.3",
    "next-connect": "^0.9.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.1",
    "rc-slider": "^9.7.1",
    "react": "^17.0.1",
    "react-awesome-reveal": "^3.5.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-icons": "^4.1.0",
    "react-loader-spinner": "^3.1.14",
    "react-number-format": "^4.4.1",
    "react-paginate": "^7.0.0",
    "react-phone-input-2": "^2.13.9",
    "react-select": "^3.1.1",
    "react-toastify": "^6.2.0",
    "react-tooltip": "^4.2.11",
    "rodal": "^1.8.1",
    "swr": "^0.3.9",
    "tailwindcss": "^2.0.2",
    "validator": "^13.5.2",
    "ws": "^7.4.2",
    "xhr2": "^0.2.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.9",
    "@types/formidable": "^1.0.32",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.11",
    "@types/nprogress": "^0.2.0",
    "@types/rc-slider": "^8.6.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-paginate": "^6.2.1",
    "@types/react-select": "^3.1.0",
    "@types/react-toastify": "^4.1.0",
    "@types/react-tooltip": "^4.2.4",
    "@types/validator": "^13.1.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "license": "MIT"
}
