{
  "name": "roompy-web",
  "version": "0.1.1",
  "description": "A free platform to connect and find roommates",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rifandani/roompy-web.git"
  },
  "bugs": {
    "url": "https://github.com/rifandani/roompy-web/issues"
  },
  "homepage": "https://github.com/rifandani/roompy-web#readme",
  "keywords": [
    "open-source",
    "nextjs",
    "firebase",
    "cypress",
    "typescript"
  ],
  "scripts": {
    "dev": "rm -rf ./.next && next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "cy:install": "cypress install",
    "cy:open": "cypress open --browser chrome",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:run-record": "cypress run --record"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@formspree/react": "^2.2.3",
    "@sentry/browser": "^6.3.1",
    "@sentry/integrations": "^6.3.1",
    "@sentry/node": "^6.3.1",
    "@sentry/webpack-plugin": "^1.15.1",
    "@turf/turf": "^6.3.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "firebase": "^8.4.1",
    "firebase-admin": "^9.6.0",
    "formik": "^2.2.6",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.3.1",
    "leaflet.markercluster": "^1.5.0",
    "localforage": "^1.9.0",
    "midtrans-client": "^1.2.3",
    "multer": "^1.4.2",
    "next": "^10.1.3",
    "next-connect": "^0.10.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.10",
    "qrcode": "^1.4.4",
    "rc-slider": "^9.7.2",
    "react": "^17.0.2",
    "react-awesome-reveal": "^3.7.1",
    "react-datepicker": "^3.7.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-icons": "^4.2.0",
    "react-leaflet": "^3.1.0",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "react-loader-spinner": "^4.0.0",
    "react-number-format": "^4.5.5",
    "react-paginate": "^7.1.2",
    "react-phone-input-2": "^2.14.0",
    "react-select": "^4.3.0",
    "react-toastify": "^7.0.3",
    "react-tooltip": "^4.2.17",
    "rodal": "^1.8.1",
    "swr": "^0.5.5",
    "tailwindcss": "^2.1.1",
    "validator": "^13.5.2",
    "ws": "^7.4.5",
    "xhr2": "^0.2.1",
    "yup": "^0.32.9",
    "yup-phone": "^1.2.19"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.1",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/leaflet": "^1.7.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.41",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.0",
    "@types/rc-slider": "^8.6.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-paginate": "^6.2.1",
    "@types/react-select": "^4.0.15",
    "@types/react-toastify": "^4.1.0",
    "@types/react-tooltip": "^4.2.4",
    "@types/validator": "^13.1.3",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cypress": "^7.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
